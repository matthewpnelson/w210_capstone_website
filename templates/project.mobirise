          "button3": "<a data-app-btn=\"true\" href=\"index.html#msg-box4-b\" class=\"btn btn-black-outline btn-black\" data-toolbar=\"-mbrBtnMove\">Generate the Perfect Brew</a>",
          "button4": "<a data-app-btn=\"true\" href=\"https://mobirise.com\" class=\"btn btn-secondary-outline\">More</a>",
